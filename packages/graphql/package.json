{
  "name": "@workspace/graphql",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "src"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./generated": "./src/generated/graphql.ts",
    "./fragments/*": "./src/fragments/*.gql",
    "./queries/*": "./src/queries/*"
  },
  "scripts": {
    "introspect": "node ./scripts/introspect.mjs",
    "codegen": "node --require ./scripts/mock-cpus.cjs ../../node_modules/@graphql-codegen/cli/cjs/bin.js --config codegen.ts"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@workspace/typescript-config": "file:../typescript-config",
    "typescript": "^5.8.2"
  }
}
