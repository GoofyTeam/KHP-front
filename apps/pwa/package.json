    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "react": "^19.0.0",
    "react-barcode-scanner": "^4.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "tailwindcss": "^4.1.10",
