{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run codegen && next dev",
    "build": "npm run codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest -w",
    "test:run": "vitest run",
    "preview": "next build && next start",
    "introspect": "get-graphql-schema https://back.goofykhp.fr/graphql > src/graphql/schema/schema.graphql",
    "codegen": "npm run introspect && graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@apollo/client-integration-nextjs": "^0.12.3",
    "@hookform/resolvers": "^5.2.1",
    "@uidotdev/usehooks": "^2.4.1",
    "@workspace/ui": "*",
    "date-fns": "^4.1.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.11.0",
    "js-cookie": "^3.0.5",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.7",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
